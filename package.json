        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.0",
        "@eslint/js": "^9.0.0",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.0.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "eslint": "^9.0.0",
        "eslint-plugin-astro": "^1.0.0",
        "globals": "^15.15.0",
        "husky": "^9.0.0",
        "rimraf": "^6.0.0",
